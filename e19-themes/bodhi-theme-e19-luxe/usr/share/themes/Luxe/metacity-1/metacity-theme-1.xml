<?xml version="1.0"?>
<metacity_theme>
        <info>
          <name>Clear-Lux 3D</name>
          <author>wmk69</author>
          <copyright>2013 wmk69</copyright>
          <date>19 Feb, 2013</date>
          <description>Modified ClearlooksMore metacity</description>
        </info>

        <frame_geometry name="normal" title_scale="medium" 
			rounded_top_left="true" 
			rounded_top_right="true" 
			rounded_bottom_left="true"  
			rounded_bottom_right="true">
          <distance name="left_width" value="5"/>
          <distance name="right_width" value="5"/>
          <distance name="bottom_height" value="5"/>
          <distance name="left_titlebar_edge" value="6"/>
          <distance name="right_titlebar_edge" value="5"/>
          <distance name="title_vertical_pad" value="4"/>
          <border name="title_border" left="5" right="5" top="3" bottom="2"/>
          <border name="button_border" left="0" right="1" top="4" bottom="3"/>
		  <!-- Buttons should be landscape oriented, i.e width >= height
		       aspect ratio - 0.75 for 4:3, 0.5625 for 16:9 -->
          <aspect_ratio name="button" value="0.75"/>
		  <!--distance name="button_width" value="22"/>
		  <distance name="button_height" value="18"/-->
        </frame_geometry>

		<frame_geometry name="normal_maximized" parent="normal" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false"  rounded_bottom_right="false">
  			<distance name="left_width" value="0"/>
  			<distance name="right_width" value="0"/>
  			<distance name="bottom_height" value="1"/>
		</frame_geometry>

        <frame_geometry name="utility" title_scale="small" parent="normal">
          <distance name="left_width" value="3"/>
          <distance name="right_width" value="3"/>
          <distance name="bottom_height" value="3"/>
          <distance name="left_titlebar_edge" value="5"/>
          <distance name="right_titlebar_edge" value="4"/>
          <distance name="title_vertical_pad" value="6"/>
          <border name="title_border" left="3" right="3" top="1" bottom="1"/>
          <border name="button_border" left="0" right="1" top="3" bottom="3"/>
        </frame_geometry>

        <frame_geometry name="border" has_title="false">
          <distance name="left_width" value="4"/>
          <distance name="right_width" value="4"/>
          <distance name="bottom_height" value="4"/>
          <distance name="left_titlebar_edge" value="0"/>
          <distance name="right_titlebar_edge" value="0"/>
          <distance name="button_width" value="0"/>
          <distance name="button_height" value="0"/>
          <distance name="title_vertical_pad" value="4"/>
          <border name="title_border" left="0" right="0" top="0" bottom="0"/>
          <border name="button_border" left="0" right="0" top="0" bottom="0"/>
        </frame_geometry>

        
        <!-- buttons -->
<!-- button minimum size / 2 -->
<constant name="Bmin" value="6"/>

<!-- button inside padding -->
<constant name="Bpad" value="8"/>

<!-- Always even area size
	width/2*2, height/2*2
-->
<draw_ops name="button_outline_normal">
<!-- main contour -->
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="3" y="1" width="width/2*2-6" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="1" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="width/2*2-2" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="3" y="height/2*2-2" width="width/2*2-6" height="1"/>
<!-- dots of main contour -->	
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="2" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="2" y="height/2*2-3" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="width/2*2-3" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.5" alpha="1"
		x="width/2*2-3" y="height/2*2-3" width="1" height="1"/>
<!-- white left inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/1.1" alpha="1"
		x="2" y="3" width="1" height="height/2*2-6"/>
<!-- dark right inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/0.93" alpha="1"
		x="width/2*2-3" y="3" width="1" height="height/2*2-6"/>
<!-- inner fill -->
	<gradient type="vertical" x="3" y="2" width="width/2*2-6" height="height/2*2-4">
		<color value="shade/gtk:bg[NORMAL]/1.07"/>
		<color value="shade/gtk:bg[NORMAL]/0.93"/>
	</gradient>
<!-- dark outer dots -->
	<!-- top -->
	<tint color="shade/gtk:bg[NORMAL]/0.95" alpha="1"
		x="2" y="1" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.95" alpha="1"
		x="1" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.95" alpha="1"
		x="width/2*2-3" y="1" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.95" alpha="1"
		x="width/2*2-2" y="2" width="1" height="1"/>
	<!-- bottom -->
	<tint color="shade/gtk:bg[NORMAL]/0.9" alpha="1"
		x="2" y="height/2*2-2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.9" alpha="1"
		x="1" y="height/2*2-3" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.9" alpha="1"
		x="width/2*2-3" y="height/2*2-2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.9" alpha="1"
		x="width/2*2-2" y="height/2*2-3" width="1" height="1"/>
<!-- top outer line and 2 dots-->
	<tint color="shade/gtk:bg[NORMAL]/1.04" alpha="1"
		x="2" y="0" width="width/2*2-4" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/1.04" alpha="1"
		x="1" y="1" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/1.04" alpha="1"
		x="width/2*2-2" y="1" width="1" height="1"/>
<!-- bottom outer line and 2 dots-->
	<tint color="shade/gtk:bg[NORMAL]/1.07" alpha="1"
		x="2" y="height/2*2-1" width="width/2*2-4" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/1.07" alpha="1"
		x="1" y="height/2*2-2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/1.07" alpha="1"
		x="width/2*2-2" y="height/2*2-2" width="1" height="1"/>
<!-- outer line-gradients -->
	<gradient type="vertical" 
		x="0" y="2" width="1" height="height/2*2-4">
		<color value="shade/gtk:bg[NORMAL]/1.04"/>
		<color value="shade/gtk:bg[NORMAL]/1.07"/>
	</gradient>
	<gradient type="vertical" 
		x="width/2*2-1" y="2" width="1" height="height/2*2-4">
		<color value="shade/gtk:bg[NORMAL]/1.04"/>
		<color value="shade/gtk:bg[NORMAL]/1.07"/>
	</gradient>
</draw_ops>

<draw_ops name="button_outline_prelight">
	<include name="button_outline_normal"/>
<!-- white left inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/1.1" alpha="1"
		x="2" y="3" width="1" height="height/2*2-6"/>
<!-- dark right inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/0.93" alpha="1"
		x="width/2*2-3" y="3" width="1" height="height/2*2-6"/>
<!-- inner fill -->
	<gradient type="vertical" 
		x="3" y="2" width="width/2*2-6" height="height/2*2-4">
		<color value="shade/gtk:bg[NORMAL]/1.3"/>
		<color value="shade/gtk:bg[NORMAL]/1.05"/>
	</gradient>
</draw_ops>

<draw_ops name="button_outline_unfocused">
	<include name="button_outline_normal"/>
<!-- main contour -->
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="3" y="1" width="width/2*2-6" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="1" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="width/2*2-2" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="3" y="height/2*2-2" width="width/2*2-6" height="1"/>
<!-- dots of main contour -->	
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="2" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="2" y="height/2*2-3" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="width/2*2-3" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.8" alpha="1"
		x="width/2*2-3" y="height/2*2-3" width="1" height="1"/>
<!-- dark outer dots -->
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="2" y="1" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="1" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="2" y="height/2*2-2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="1" y="height/2*2-3" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="width/2*2-3" y="1" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="width/2*2-2" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="width/2*2-3" y="height/2*2-2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.96" alpha="1"
		x="width/2*2-2" y="height/2*2-3" width="1" height="1"/>
</draw_ops>

<draw_ops name="button_outline_pressed">
	<include name="button_outline_normal"/>
<!-- main contour -->
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="3" y="1" width="width/2*2-6" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="1" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="width/2*2-2" y="3" width="1" height="height/2*2-6"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="3" y="height/2*2-2" width="width/2*2-6" height="1"/>
<!-- dots of main contour -->	
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="2" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="2" y="height/2*2-3" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="width/2*2-3" y="2" width="1" height="1"/>
	<tint color="shade/gtk:bg[NORMAL]/0.6" alpha="1"
		x="width/2*2-3" y="height/2*2-3" width="1" height="1"/>
<!-- left inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/0.80" alpha="1"
		x="2" y="3" width="1" height="height/2*2-6"/>
<!-- right inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/0.80" alpha="1"
		x="width/2*2-3" y="3" width="1" height="height/2*2-6"/>
<!-- inner fill -->
	<gradient type="vertical" 
		x="3" y="2" width="width/2*2-6" height="height/2*2-4">
		<color value="shade/gtk:bg[NORMAL]/0.94"/>
		<color value="shade/gtk:bg[NORMAL]/0.94"/>
	</gradient>
<!-- top inner outline -->
	<tint color="shade/gtk:bg[NORMAL]/0.80" alpha="1" 
		x="3" y="2" width="width/2*2-6" height="1"/>
</draw_ops>

<!-- buttons focused -->
<draw_ops name="button_minimize_focused">
	<line color="gtk:fg[NORMAL]" 
		x1="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1-1" 
		x2="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2  + ((height-(Bpad*2))`max`(Bmin*2)) -1" 
		y2="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1-1"/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1" 
		x2="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2  + ((height-(Bpad*2))`max`(Bmin*2)) -1" 
		y2="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1"/>
</draw_ops>
<draw_ops name="button_maximize_focused">
	<rectangle color="gtk:fg[NORMAL]" filled="false" 
		x="( width- ((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1" 
		y="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2+2-1"
		width="((height-(Bpad*2))`max`(Bmin*2))-1" 
		height="((height-(Bpad*2))`max`(Bmin*2))-1-1"/>
	<rectangle color="gtk:fg[NORMAL]" filled="false" 
		x="( width- ((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1" 
		y="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1"
		width="((height-(Bpad*2))`max`(Bmin*2))-1" 
		height="((height-(Bpad*2))`max`(Bmin*2))-1"/>
</draw_ops>
<draw_ops name="button_close_focused">
	<!-- ((height-Bpad*2)`max`(Bmin*2))  - line Y size
	     ( height- ((height-Bpad*2)`max`(Bmin*2)) )/2  - Y padding
	     ( width-  ((height-Bpad*2)`max`(Bmin*2)) )/2  - X padding -->
	<!-- upper X -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+2-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1-1"
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1-1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1-1" 
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+1-1"
	/>
	<!-- central X -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1"
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+1-1"
	/>
	<!-- lower X -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+2-1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1-1" 
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1"
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2+2-1" 
		y1="( height-((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
		x2="( width- ((height-Bpad*2)`max`(Bmin*2)) )/2 + ((height-Bpad*2)`max`(Bmin*2)) -1" 
		y2="( height-((height-Bpad*2)`max`(Bmin*2)) )/2+2-1"
	/>
</draw_ops>
<draw_ops name="button_menu_focused">
	<!-- ((height-Bpad*2)/2`max`Bmin)  - line Y dimension 
	     ( height- ((height-Bpad*2)/2`max`Bmin) )/2  - Y padding 
	     ( width-((height-Bpad*2)/2`max`Bmin)*2 )/2  - X padding -->
	<!-- upper V -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2+2-1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2+1-1" 
		x2="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2 + ((height-Bpad*2)/2`max`Bmin) +1-1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin) -1"
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin) -1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin) -1" 
		x2="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin)*2 -1-1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2+1-1"
	/>
	<!-- central V -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2+2-1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2+2-1" 
		x2="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2 + ((height-Bpad*2)/2`max`Bmin) +1-1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin) +1-1"
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin) -1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin)+1-1" 
		x2="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin)*2 -1-1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2+2-1"
	/>
	<!-- lower V -->
	<line color="gtk:fg[NORMAL]" 
		x1="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2+1-1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2+1+1-1" 
		x2="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2 + ((height-Bpad*2)/2`max`Bmin) -1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin)+1 -1"
	/>
	<line color="gtk:fg[NORMAL]" 
		x1="( width -((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin) +1 -1" 
		y1="( height-((height-Bpad*2)/2`max`Bmin) )/2 + ((height-Bpad*2)/2`max`Bmin) +1 -1" 
		x2="( width- ((height-Bpad*2)/2`max`Bmin)*2 )/2  + ((height-Bpad*2)/2`max`Bmin)*2 -1" 
		y2="( height-((height-Bpad*2)/2`max`Bmin) )/2+1+1-1"
	/>
</draw_ops>

<!-- buttons prelight -->
<draw_ops name="button_minimize_prelight">
	<line color="gtk:fg[SELECTED]" 
		x1="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2+1-1" 
		y1="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1-1" 
		x2="( width-((height-(Bpad*2))`max`(Bmin*2)) )/2  + ((height-(Bpad*2))`max`(Bmin*2)) -1" 
		y2="( height-((height-(Bpad*2))`max`(Bmin*2)) )/2 + ((height-(Bpad*2))`max`(Bmin*2)) -1-1"/>
	<line color="gtk:fg[SELECTED]" 
